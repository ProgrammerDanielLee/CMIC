<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/meeting_place_toolbar_color"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="매칭 결과 확인"
        android:textStyle="bold"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:textSize="20dp"
        android:textColor="@color/meeting_place_toolbar_font_color"
        android:gravity="center"/>

    <ListView
        android:id="@+id/core_one_list_result"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_gravity="center_horizontal"
        android:layout_weight="80"
        android:listSelector="@color/list_item_select"
        android:fontFamily="sans-serif"
        android:divider="@color/meeting_place_toolbar_font_color"
        />


    </LinearLayout>