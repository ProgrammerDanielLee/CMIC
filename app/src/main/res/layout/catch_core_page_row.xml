        android:visibility="gone"
        android:id="@+id/row_core_img2"
        android:layout_marginLeft="-20dp"
        android:layout_marginRight="-20dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_weight="240"
        android:src="@drawable/core_icon_select_twodepth"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:gravity="center"
        />
    <ImageView
        android:visibility="gone"
        android:id="@+id/row_core_img3"
        android:layout_marginLeft="-20dp"
        android:layout_marginRight="-20dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_weight="240"
        android:src="@drawable/core_icon_select_threedepth"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:gravity="center"
        />
    <ImageView
        android:visibility="gone"
        android:id="@+id/row_core_img4"
        android:layout_marginLeft="-20dp"
        android:layout_marginRight="-20dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_weight="240"
        android:src="@drawable/core_icon_result_onedepth"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:gravity="center"
        />
    <ImageView
        android:visibility="gone"
        android:id="@+id/row_core_img5"
        android:layout_marginLeft="-20dp"
        android:layout_marginRight="-20dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_weight="240"
        android:src="@drawable/core_icon_result_twodepth"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:gravity="center"
        />
    <ImageView
        android:visibility="gone"
        android:id="@+id/row_core_img6"
        android:layout_marginLeft="-20dp"
        android:layout_marginRight="-20dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_weight="240"
        android:src="@drawable/core_icon_result_threedepth"
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:gravity="center"
        />
    <!--두번째 것 시작-->
    <LinearLayout
        android:layout_marginTop="10dp"
        android:layout_weight="380"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <ImageView
            android:layout_gravity="center"
            android:layout_weight="1"
            android:id="@+id/pro_img_right"
            android:layout_width="60dp"
            android:layout_height="60dp"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="10">
            <TextView
                android:textColor="@color/meeting_place_toolbar_font_color"
